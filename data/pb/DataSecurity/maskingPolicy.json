{
    "tenantId" : "pb",
    "moduleName" : "DataSecurity",
    "MaskingPolicy":[
        {
            "key": "mobileNumber",
            "pattern": ".(?=.{4})"
        },
        {
            "key": "altContactNumber",
            "pattern": ".(?=.{4})"
        },
        {
            "key": "permanentAddress",
            "pattern": "\\B[a-z]"
        },
        {
            "key": "permanentCity",
            "pattern": "\\B[a-z]"
        },
        {
            "key": "permanentPinCode",
            "pattern": ".(?=.{2})"
        },
        {
            "key": "correspondenceAddress",
            "pattern": "\\B[a-z]"
        },
        {
            "key": "correspondenceCity",
            "pattern": "\\B[a-z]"
        },
        {
            "key": "correspondencePinCode",
            "pattern": ".(?=.{2})"
        },
        {
            "key": "emailId",
            "pattern": "(?<=.)[^@\\n](?=[^@\\n]*?@)|(?:(?<=@.)|(?!^)\\G(?=[^@\\n]*$)).(?!$)"
        },
        {
            "key": "name",
            "pattern": "\\B[a-z]"
        },
        {
            "key": "password",
            "pattern": "[A-Za-zÀ-ȕ0-9(),-_., ]"
        },
        {
            "key": "gender",
            "pattern": "\\w(?=(?:[ \\w]*\\w){2}$)"
        },
        {
            "key": "userName",
            "pattern": "\\B[a-z]"
        },
        {
            "key": "dob",
            "pattern": ".(?=.{4})"
        }
    ]
}