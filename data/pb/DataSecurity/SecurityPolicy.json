{
  "tenantId": "pb",
  "moduleName": "DataSecurity",
  "SecurityPolicy": [
    {
      "model": "User",
      "uniqueIdentifier": {
        "name": "uuid",
        "jsonPath": "uuid"
      },
      "attributes": [
        {
          "name": "name",
          "jsonPath": "name",
          "patternId": "002",
          "defaultVisibility": "MASKED"
        },
        {
          "name": "mobileNumber",
          "jsonPath": "mobileNumber",
          "patternId": "001",
          "defaultVisibility": "MASKED"
        },
        {
          "name": "emailId",
          "jsonPath": "emailId",
          "patternId": "003",
          "defaultVisibility": "MASKED"
        },
        {
          "name": "username",
          "jsonPath": "username",
          "defaultVisibility": "NONE"
        },
        {
          "name": "altContactNumber",
          "jsonPath": "altContactNumber",
          "defaultVisibility": "NONE"
        },
        {
          "name": "pan",
          "jsonPath": "pan",
          "defaultVisibility": "NONE"
        },
        {
          "name": "aadhaarNumber",
          "jsonPath": "aadhaarNumber",
          "defaultVisibility": "NONE"
        },
        {
          "name": "guardian",
          "jsonPath": "guardian",
          "defaultVisibility": "NONE"
        },
        {
          "name": "permanentAddress",
          "jsonPath": "permanentAddress/address",
          "defaultVisibility": "NONE"
        },
        {
          "name": "correspondenceAddress",
          "jsonPath": "correspondenceAddress/address",
          "defaultVisibility": "NONE"
        },
        {
          "name": "addresses",
          "jsonPath": "addresses/*/address",
          "defaultVisibility": "NONE"
        }
      ],
      "roleBasedDecryptionPolicy": [
        {
          "role": "PGR_LME",
          "attributeAccessList": [
            {
              "attribute": "name",
              "secondLevelVisibility": "PLAIN"
            },
            {
              "attribute": "mobileNumber",
              "secondLevelVisibility": "PLAIN"
            }
          ]
        },
        {
          "role": "GRO",
          "attributeAccessList": [
            {
              "attribute": "mobileNumber",
              "firstLevelVisibility": "MASKED"
            }
          ]
        }
      ]
    }
  ]
}
